CREATE TABLE IF NOT EXISTS src_raw_from_mongodb_test (
	
						_id VARCHAR(100) PRIMARY KEY,
						_type VARCHAR(50),
						date_extracted date,
                        id integer,
						adId VARCHAR(250),
						adSource VARCHAR(250),
                        featured boolean,
                        name VARCHAR(300),
                        brand VARCHAR(300),
                        description VARCHAR(300),
                        size VARCHAR(250),
                        availability boolean,
						availabilityType text,
						imageUris text,
						locations text,
						onlineHeirs text,
						"restrictions.retailLimit" smallint,
						"restrictions.promotionalLimit" smallint,
						"restrictions.liquorAgeRestrictionFlag" boolean,
						"restrictions.tobaccoAgeRestrictionFlag" boolean,
						"restrictions.restrictedByOrganisation" boolean,
						"restrictions.delivery" text,
                        "merchandiseHeir.tradeProfitCentre" VARCHAR(250),
                        "merchandiseHeir.categoryGroup" VARCHAR(250),
                        "merchandiseHeir.category" VARCHAR(250),
                        "merchandiseHeir.subCategory" VARCHAR(250),
                        "merchandiseHeir.className" VARCHAR(250),
                        "pricing.now" decimal,
						"pricing.was" decimal,
                        "pricing.unit.quantity" smallint,
                        "pricing.unit.ofMeasureQuantity" smallint,
                        "pricing.unit.ofMeasureUnits" VARCHAR(50),
                        "pricing.unit.price" decimal,
                        "pricing.unit.ofMeasureType" VARCHAR(50),
                        "pricing.unit.isWeighted" boolean,
                        "pricing.comparable" VARCHAR(100),
                        "pricing.onlineSpecial" boolean,
                        "pricing.promotionType" VARCHAR(250),
						internalDescription VARCHAR(250),
                        "pricing.specialType" VARCHAR(250),
                        "pricing.offerDescription" VARCHAR(250),
                        "pricing.multiBuyPromotion.type" VARCHAR(250),
                        "pricing.multiBuyPromotion.id" integer,
                        "pricing.multiBuyPromotion.minQuantity" smallint,
                        "pricing.multiBuyPromotion.reward" decimal,
                        "pricing.promotionDescription" VARCHAR(250)
)
